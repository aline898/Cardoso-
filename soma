#include <stdio.h>

int main() {
    int num1, num2, soma;

    // Solicita ao usuário que insira dois números
    printf("Digite o primeiro numero: ");
    scanf("%d", &num1);
    printf("Digite o segundo numero: ");
    scanf("%d", &num2);

    // Calcula a soma dos dois números
    soma = num1 + num2;

    // Exibe o resultado da soma
    printf("A soma dos numeros e: %d\n", soma);

    return 0;
}
